<p id="notice"><%= notice %></p>

<h1>Listing Pdffolders</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pdffolders.each do |pdffolder| %>
      <tr>
        <td><%= pdffolder.name %></td>
        <td><%= link_to 'Show', pdffolder %></td>
        <td><%= link_to 'Edit', edit_pdffolder_path(pdffolder) %></td>
        <td><%= link_to 'Destroy', pdffolder, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pdffolder', new_pdffolder_path %>
